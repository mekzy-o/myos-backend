{
  "name": "myos-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn build-ts",
    "build-ts": "rimraf build && tsc",
    "start:dev": "DEBUG=development && tsc-watch --onSuccess \"nodemon build/index.js\"",
    "start": "node build/index.js",
    "format": "prettier --write \".md\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:reset": "yarn run db:migrate:undo && yarn run db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.2",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "pg": "^8.3.2",
    "sequelize": "4",
    "sequelize-cli": "^6.2.0",
    "slug": "^5.3.0",
    "tsc-watch": "^4.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/sequelize": "4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}